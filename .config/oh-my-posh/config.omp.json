{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#1C314F",
          "foreground": "#669efa",
          "style": "diamond",
          "template": " {{ .Icon }} ",
          "trailing_diamond": "\ue0b0",
          "type": "os"
        },
        {
          "background": "#DE2121",
          "foreground": "#FFFFFF",
          "leading_diamond": "<transparent,#DE2121>\ue0b0</>",
          "style": "diamond",
          "template": " \uf0e7 ",
          "trailing_diamond": "\ue0b0",
          "type": "root"
        },
        {
          "background": "#333333",
          "foreground": "#FFFFFF",
          "leading_diamond": "<transparent,#333333>\ue0b0</>",
          "properties": {
            "folder_icon": "~",
            "folder_separator_icon": "<transparent> \ue0b1 </>",
            "home_icon": "\ueb06",
            "style": "agnoster_short"
          },
          "style": "diamond",
          "template": " {{ .Path }} ",
          "trailing_diamond": "\ue0b0",
          "type": "path"
        },
        {
          "background": "#669efa",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#669efa{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#669efa{{ end }}",
            "{{ if gt .Ahead 0 }}#669efa{{ end }}",
            "{{ if gt .Behind 0 }}#669efa{{ end }}"
          ],
          "foreground": "#1C314F",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#06A4CE",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "<background,transparent>\ue0b2</> {{ .Full }} \ue798 <transparent,background>\ue0b2</>",
          "type": "dart"
        },
        {
          "background": "#00ffff",
          "foreground": "#000000",
          "invert_powerline": true,
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "<background,transparent>\ue0b2</> {{ .Full }} \ue77f <transparent,background>\ue0b2</>",
          "type": "dotnet"
        },
        {
          "background": "#4063D8",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "<background,transparent>\ue0b2</> {{ .Full }} \ue738 <transparent,background>\ue0b2</>",
          "type": "java"
        },
        {
          "background": "#906cff",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "<background,transparent>\ue0b2</> {{ .Full }} <b>K</b> <transparent,background>\ue0b2</>",
          "type": "kotlin"
        },
        {
          "background": "#6CA35E",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "<background,transparent>\ue0b2</> {{ .Full }} \ue718 <transparent,background>\ue0b2</>",
          "type": "node"
        },
        {
          "background": "#4063D8",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "properties": {
            "fetch_version": true
          },
          "style": "powerline",
          "template": "<background,transparent>\ue0b2</> {{ .Full }} \ue73d <transparent,background>\ue0b2</>",
          "type": "php"
        },
        {
          "background": "#906cff",
          "foreground": "#100e23",
          "invert_powerline": true,
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "powerline",
          "template": "<background,transparent>\ue0b2</> {{ .Full }} \ue235 <transparent,background>\ue0b2</>",
          "type": "python"
        },
        {
          "background": "#99908a",
          "foreground": "#193549",
          "invert_powerline": true,
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "powerline",
          "template": "<background,transparent>\ue0b2</> {{ .Full }} \ue7a8 <transparent,background>\ue0b2</>",
          "type": "rust"
        },
        {
          "background": "#FEAC19",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "powerline",
          "template": "<background,transparent>\ue0b2</> {{ .Full }} \uf0e7 <transparent,background>\ue0b2</>",
          "type": "azfunc"
        },
        {
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "foreground": "#ffffff",
          "invert_powerline": true,
          "properties": {
            "display_default": false
          },
          "style": "powerline",
          "template": "<background,transparent>\ue0b2</> {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} Óû≠ <transparent,background>\ue0b2</>",
          "type": "aws"
        },
        {
          "background": "#333333",
          "foreground": "#ffffff",
          "invert_powerline": true,
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": "<background,transparent>\ue0b2</> {{ .FormattedMs }} \uf252 <transparent,background>\ue0b2</>",
          "type": "executiontime"
        },
        {
          "background": "#1C314F",
          "background_templates": ["{{ if gt .Code 0 }}#cc2222{{ end }}"],
          "foreground": "#669efa",
          "foreground_templates": ["{{ if gt .Code 0 }}#ffffff{{ end }}"],
          "invert_powerline": true,
          "properties": {
            "always_enabled": true
          },
          "style": "powerline",
          "template": "<background,transparent>\ue0b2</> {{ if gt .Code 0 }}{{ reason .Code }}{{ else }}\uf42e{{ end }} ",
          "type": "status"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{if .Root}} \u26a1 {{end}}{{.Folder | replace \"~\" \"üè†\"}} @ {{.HostName}}",
  "final_space": true,
  "version": 2
}
